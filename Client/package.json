{
  "name": "sociallogin",
  "author": "CJ <chengj@ticketek.com.au>",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build-vendor": "./node_modules/.bin/webpack --config webpack.config.vendor.js --mode development",
    "run-site": "./node_modules/.bin/webpack-dev-server -d --port 3000 --hot --history-api-fallback --open",
    "start": "npm run build-vendor && npm run run-site",
    "build-vendor-production": "./node_modules/.bin/webpack --config webpack.config.vendor.js --mode production",
    "build-site-production": "./node_modules/.bin/webpack --mode production",
    "build": "npm run build-vendor-production && npm run build-site-production",
    "clean": "rimraf node_modules/ && rimraf dist/ && del package-lock.json"
  },
  "devDependencies": {
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "3.5.1",
    "webpack-dev-server": "3.1.3",
    "webpack-hot-middleware": "2.21.2"
  },
  "dependencies": {
    "@types/classnames": "2.2.3",
    "@types/history": "4.6.2",
    "@types/react": "16.3.8",
    "@types/react-dom": "16.0.5",
    "@types/react-hot-loader": "3.0.6",
    "@types/react-redux": "5.0.16",
    "@types/react-router-dom": "4.2.6",
    "@types/react-router-redux": "5.0.13",
    "@types/redux-form": "7.2.3",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.0.0",
    "classnames": "2.2.5",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.9",
    "material-ui": "1.0.0-beta.41",
    "material-ui-icons": "1.0.0-beta.36",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-hot-loader": "4.0.1",
    "redux": "3.7.2",
    "redux-form": "7.3.0",
    "redux-thunk": "2.2.0",
    "typescript": "2.8.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14"
  }
